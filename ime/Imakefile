#define IHaveSubdirs

IMDLIB =  -LIMdkit -lXimd -L/mnt/y/xorg/mingw2/buildcygwin/exports/lib -lWinIME -lXext
DEPLIBS = $(DEPXONLYLIB)
INCLUDES = -IIMdkit -I/mnt/y/xorg/mingw2/buildcygwin/exports/include
LOCAL_LIBRARIES = $(IMDLIB) $(XONLYLIB)
SRCS = xime.c winIC.c
OBJS = xime.o winIC.o
CDEBUGFLAGS = -g
PROGRAM = xime
SUBDIRS = IMdkit

ComplexProgramTarget(xime)

MakeSubdirs($(SUBDIRS))
DependSubdirs($(SUBDIRS))
